{
  "name": "servicenow-cloudobservability-datasource",
  "version": "3.3.0",
  "description": "A Grafana datasource for ServiceNow Cloud Observability",
  "author": "ServiceNow Cloud Observability",
  "license": "Apache-2.0",
  "engines": {
    "node": "16"
  },
  "scripts": {
    "build": "grafana-toolkit plugin:build",
    "dev": "grafana-toolkit plugin:dev",
    "watch": "grafana-toolkit plugin:dev --watch",
    "sign": "grafana-toolkit plugin:sign",
    "format": "prettier --write src",
    "test": "grafana-toolkit plugin:test",
    "generate:grammar": "lezer-generator grammars/uql.grammar -o src/grammars/generated/uql"
  },
  "dependencies": {
    "@codemirror/commands": "6.3.2",
    "@codemirror/language": "6.9.3",
    "@codemirror/lint": "6.4.2",
    "@codemirror/state": "6.3.2",
    "@codemirror/view": "6.22.1",
    "@grafana/data": "9.5.3",
    "@grafana/runtime": "9.5.3",
    "@grafana/ui": "9.5.3",
    "@lezer/common": "1.1.1",
    "@lezer/highlight": "1.2.0",
    "@lezer/lr": "1.3.14",
    "react": "17.0.2",
    "react-dom": "17.0.2"
  },
  "devDependencies": {
    "@grafana/toolkit": "9.5.3",
    "@lezer/generator": "1.5.1",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "12.1.5",
    "tiny-invariant": "1.3.1"
  }
}
