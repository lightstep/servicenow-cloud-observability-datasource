{
  "name": "servicenow-cloudobservability-datasource",
  "version": "4.1.1",
  "description": "A Grafana datasource for ServiceNow Cloud Observability",
  "author": "ServiceNow Cloud Observability",
  "license": "Apache-2.0",
  "engines": {
    "node": ">=20"
  },
  "scripts": {
    "build": "webpack -c ./.config/webpack/webpack.config.ts --env production",
    "dev": "webpack -w -c ./.config/webpack/webpack.config.ts --env development",
    "test": "jest --watch --onlyChanged",
    "typecheck": "tsc --noEmit",
    "lint": "eslint --cache --ignore-path ./.gitignore --ext .js,.jsx,.ts,.tsx .",
    "sign": "npx --yes @grafana/sign-plugin@latest",
    "format": "prettier --write src",
    "generate:grammar": "lezer-generator grammars/uql.grammar -o src/grammars/generated/uql"
  },
  "dependencies": {
    "@codemirror/commands": "6.3.2",
    "@codemirror/language": "6.9.3",
    "@codemirror/lint": "6.4.2",
    "@codemirror/state": "6.3.2",
    "@codemirror/view": "6.22.1",
    "@emotion/css": "11.10.6",
    "@grafana/data": "10.0.3",
    "@grafana/runtime": "10.0.3",
    "@grafana/schema": "10.0.3",
    "@grafana/ui": "10.0.3",
    "@lezer/common": "1.1.1",
    "@lezer/highlight": "1.2.0",
    "@lezer/lr": "1.3.14",
    "lodash": "^4.17.21",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "tslib": "2.5.3"
  },
  "devDependencies": {
    "@babel/core": "^7.21.4",
    "@grafana/eslint-config": "^6.0.0",
    "@grafana/tsconfig": "^1.2.0-rc1",
    "@lezer/generator": "1.5.1",
    "@swc/core": "^1.3.90",
    "@swc/helpers": "^0.5.0",
    "@swc/jest": "^0.2.26",
    "@testing-library/jest-dom": "6.1.4",
    "@testing-library/react": "14.0.0",
    "@types/jest": "^29.5.0",
    "@types/lodash": "^4.14.194",
    "@types/node": "^20.8.7",
    "@types/testing-library__jest-dom": "5.14.8",
    "copy-webpack-plugin": "^11.0.0",
    "css-loader": "^6.7.3",
    "eslint-plugin-deprecation": "^2.0.0",
    "eslint-webpack-plugin": "^4.0.1",
    "fork-ts-checker-webpack-plugin": "^8.0.0",
    "glob": "^10.2.7",
    "identity-obj-proxy": "3.0.0",
    "jest": "^29.5.0",
    "jest-environment-jsdom": "^29.5.0",
    "prettier": "^2.8.7",
    "replace-in-file-webpack-plugin": "^1.0.6",
    "sass": "1.63.2",
    "sass-loader": "13.3.1",
    "style-loader": "3.3.3",
    "swc-loader": "^0.2.3",
    "tiny-invariant": "1.3.1",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "typescript": "4.8.4",
    "webpack": "^5.86.0",
    "webpack-cli": "^5.1.4",
    "webpack-livereload-plugin": "^3.0.2"
  }
}
